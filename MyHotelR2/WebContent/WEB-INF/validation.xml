<!DOCTYPE form-validation PUBLIC
          "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
          "http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">
<form-validation>     
<formset>
  <form name="loginForm">
    <field property="userId" depends="required">
      <arg key="user.id"/>
    </field>
    <field property="password" depends="required">
      <arg key="user.password" />
    </field>
  </form>
  <form name="addadminForm">
    <field property="adminId" depends="required">
      <arg key="admin.id"/>
    </field>
    <field property="password" depends="required">
      <arg key="admin.password" />
    </field>
    <field property="name" depends="required">
      <arg key="admin.name" />
    </field>
  </form>
  <form name="addroomtypeForm">
    <field property="typename" depends="required">
      <arg key="roomtype.typename"/>
    </field>
    <field property="price" depends="required">
      <arg key="roomtype.price"/>
    </field>
  </form>
  <form name="addroomForm">
    <field property="roomId" depends="required">
      <arg key="room.roomId"/>
    </field>
    <field property="typeId" depends="required">
      <arg key="room.typeId"/>
    </field>
    <field property="floor" depends="required">
      <arg key="room.floor"/>
    </field>
  </form>
  <form name="addbadmanForm">
    <field property="name" depends="required">
      <arg key="badman.name"/>
    </field>
    <field property="cardno" depends="required">
      <arg key="badman.cardno"/>
    </field>
  </form>
  <form name="addclientForm">
    <field property="clientId" depends="required">
      <arg key="client.clientId"/>
    </field>
    <field property="password" depends="required">
      <arg key="client.password"/>
    </field>
    <field property="rpassword" depends="required">
      <arg key="client.rpassword"/>
    </field>
    <field property="name" depends="required">
      <arg key="client.name"/>
    </field>
    <field property="date1" depends="required">
      <arg key="client.birth"/>
    </field>
    <field property="cardno" depends="required">
      <arg key="client.cardno"/>
    </field>
  </form>
  <form name="clientInfoForm">
    <field property="clientId" depends="required">
      <arg key="client.clientId"/>
    </field>
    <field property="password" depends="required">
      <arg key="client.password"/>
    </field>
    <field property="name" depends="required">
      <arg key="client.name"/>
    </field>
    <field property="date1" depends="required">
      <arg key="client.birth"/>
    </field>
    <field property="cardno" depends="required">
      <arg key="client.cardno"/>
    </field>
  </form>
  <form name="addorder2Form">
    <field property="name" depends="required">
      <arg key="order2.name"/>
    </field>
    <field property="date1" depends="required">
      <arg key="order2.birth"/>
    </field>
    <field property="cardno" depends="required">
      <arg key="order2.cardno"/>
    </field>
    <field property="roomId" depends="required">
      <arg key="order2.roomId"/>
    </field>
    <field property="datenum" depends="required">
      <arg key="order2.datenum"/>
    </field>
  </form>
  <form name="tongjiForm">
    <field property="date1" depends="required">
      <arg key="tongji.date1"/>
    </field>
    <field property="date2" depends="required">
      <arg key="tongji.date2"/>
    </field>
  </form>
</formset>
</form-validation>